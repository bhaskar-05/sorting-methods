#include <iostream>
using namespace std;

int main() {
    
    int arr[5]={1,5,2,9,8};
    int i=0;
    int j=i+1;
    while(i<5 && j<5) {
        
        if(arr[i]> arr[j]) {
            
            swap(arr[i],arr[j]);
        }
        i+=1;
        j+=1;
    }
    for(int i=0;i<5;i++) {
        
        cout << arr[i] << " ";
    }
}
