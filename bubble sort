#include <iostream>
using namespace std;

int main() {
    int n=4;
    int arr[4]={4,3,2,1};
    for (int i=0; i<n-1; i++) {
        for (int j=0; j<n;j++ ) { //as we know in bubble sort in ith round ith largest element gets sorted therfore we neet not run j everytime till end else we can simply put j<n-i
            
            if (arr[j]> arr[j+1]) {
                
                swap(arr[j],arr[j+1]);
            }
        }
    }
        
        
    
    for(int i=0;i<4;i++) {
        
        cout << arr[i] << " ";
    }
}
